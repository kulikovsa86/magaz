= render '/magaz/categories/breadcrumb'

.panel.panel-default
  .panel-heading
    %h4.panel_header= @product.name

  .panel-body
    = render '/magaz/products/tabs'
    %h4= t('.title')
    %br
    .row
      .col-md-12
        = render 'form'

= link_to t('helpers.link_to.back'), product_variants_path(@product)