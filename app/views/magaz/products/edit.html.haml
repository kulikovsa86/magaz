
= render '/magaz/categories/breadcrumb'

.panel.panel-default
  .panel-heading
    %h4.panel_header
      - unless @product.name.empty?
        = @product.name
      - else
        = t('.default_header')
  .panel-body
    = render 'tabs'
    %br
    = render 'form'
      
= link_to t('helpers.link_to.back'), category_products_path(@parent_category)
